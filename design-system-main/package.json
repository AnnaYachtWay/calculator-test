{
  "name": "@yachtway/design-system",
  "version": "0.5.6",
  "description": "Shared package for common components and MUI setup",
  "author": "YachtWay",
  "private": true,
  "license": "UNLICENSED",
  "main": "src/components/index.ts",
  "module": "src/components/index.ts",
  "types": "src/components/index.ts",
  "files": [
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/yachtway/design-system.git"
  },
  "homepage": "https://github.com/yachtway/design-system#readme",
  "bugs": {
    "url": "https://github.com/yachtway/design-system/issues"
  },
  "scripts": {
    "build:constants": "tsc --project src/theme/constants/tsconfig.json && prettier --write \"./src/theme/constants/*.{js,jsx,ts,tsx}\" && eslint --fix ./src/theme/constants",
    "lint": "prettier --check \"./src/**/*.{js,jsx,ts,tsx}\" && eslint . && tsc --noEmit",
    "lint:fix": "prettier --write \"./src/**/*.{js,jsx,ts,tsx}\" && eslint --fix . && tsc --noEmit",
    "stylelint": "stylelint ./src/**/*.{css,scss,sass,less}",
    "staged-prettier-format": "prettier --write",
    "staged-lint-fix": "eslint --fix",
    "staged-stylelint-fix": "stylelint --fix",
    "prepare": "husky",
    "release:patch": "yarn version --patch && git push origin main --tags",
    "release:minor": "yarn version --minor && git push origin main --tags",
    "release:major": "yarn version --major && git push origin main --tags"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "npm run staged-prettier-format",
      "npm run staged-lint-fix"
    ],
    "*.{css,scss,sass,less}": [
      "npm run staged-prettier-format",
      "npm run staged-stylelint-fix"
    ]
  },
  "peerDependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@emotion/react": "^11.14.0",
    "@mui/icons-material": "^5.15.11",
    "@mui/material": "^5.15.11",
    "dayjs": "^1.11.13",
    "framer-motion": "^12.17.3",
    "heic2any": "^0.0.4",
    "next": "14.2.26",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.57.0",
    "react-number-format": "^5.4.4",
    "react-phone-input-2": "^2.15.1",
    "tailwind-merge": "^3.3.1"
  },
  "dependencies": {
    "react-datepicker": "^8.4.0"
  },
  "devDependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@emotion/react": "^11.14.0",
    "@mui/icons-material": "^5.15.11",
    "@mui/material": "^5.15.11",
    "@savvywombat/tailwindcss-grid-areas": "^4.0.0",
    "@swc/core": "^1.12.1",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^18.2.18",
    "@yachtway/eslint-config-react": "git+https://github.com/yachtway/fe-eslint-config#v1.1.10",
    "dayjs": "^1.11.13",
    "eslint": "^9.36.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "framer-motion": "^12.17.3",
    "heic2any": "^0.0.4",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "next": "14.2.26",
    "postcss": "^8.4.35",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.5.13",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.57.0",
    "react-number-format": "^5.4.4",
    "react-phone-input-2": "^2.15.1",
    "stylelint": "^16.2.1",
    "stylelint-config-idiomatic-order": "^10.0.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-sass-guidelines": "^11.0.0",
    "stylelint-config-standard": "^36.0.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^3.4.1",
    "tailwindcss-text-fill": "^0.2.0",
    "typescript": "^5.8.3"
  }
}
